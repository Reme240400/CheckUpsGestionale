<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextArea?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="467.0" prefWidth="641.0" xmlns="http://javafx.com/javafx/20.0.1"
   xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.Controllers.CreazioneUnitaLocale">
   <center>
      <HBox alignment="CENTER">
         <children>
            <VBox alignment="CENTER" prefHeight="252.0" prefWidth="641.0">
               <children>
                  <Pane prefHeight="200.0" prefWidth="200.0" />
                  <AnchorPane>
                     <children>
                        <Label layoutX="14.0" layoutY="9.0" prefHeight="17.0" prefWidth="98.0"
                           text="Unità Produttiva:" />
                        <TextField fx:id="textFieldUnitaLocale" layoutX="123.0" layoutY="5.0"
                           onKeyReleased="#keyReleasedProperty" prefHeight="25.0" prefWidth="412.0" />
                     </children>
                  </AnchorPane>
                  <Pane prefHeight="200.0" prefWidth="200.0" />
                  <AnchorPane>
                     <children>
                        <Label layoutX="14.0" layoutY="6.0" prefHeight="17.0" prefWidth="56.0"
                           text="Località:" />
                        <TextField fx:id="textFieldLocalita" layoutX="82.0" layoutY="1.0"
                           onKeyReleased="#keyReleasedProperty" prefHeight="25.0" prefWidth="323.0" />
                        <Label layoutX="458.0" layoutY="6.0" prefHeight="17.0" prefWidth="56.0"
                           text="Provincia:" />
                        <TextField fx:id="textFieldProvincia" accessibleText="INTEGER"
                           layoutX="522.0" layoutY="2.0" onKeyReleased="#keyReleasedProperty"
                           prefHeight="25.0" prefWidth="56.0" />
                     </children>
                  </AnchorPane>
                  <Pane prefHeight="200.0" prefWidth="200.0" />
                  <AnchorPane>
                     <children>
                        <Label layoutX="14.0" layoutY="9.0" prefHeight="17.0" prefWidth="56.0"
                           text="Indirizzo:" />
                        <TextField fx:id="textFieldIndirizzo" layoutX="83.0" layoutY="5.0"
                           onKeyReleased="#keyReleasedProperty" prefHeight="25.0" prefWidth="323.0" />
                     </children>
                  </AnchorPane>
                  <Pane prefHeight="200.0" prefWidth="200.0" />
                  <AnchorPane disable="true">
                     <children>
                        <Label layoutX="14.0" layoutY="9.0" prefHeight="17.0" prefWidth="56.0"
                           text="Telefono:" />
                        <TextField fx:id="textFieldTel" layoutX="85.0" layoutY="7.0"
                           onKeyReleased="#keyReleasedProperty" prefHeight="25.0" prefWidth="323.0" />
                     </children>
                  </AnchorPane>
                  <Pane prefHeight="200.0" prefWidth="200.0" />
                  <AnchorPane prefHeight="55.0" prefWidth="674.0">
                     <children>
                        <Label layoutX="14.0" text="Cerca / Carica da File">
                           <font>
                              <Font name="Bell MT Bold" size="21.0" />
                           </font>
                        </Label>
                        <JFXComboBox editable="true" layoutX="14.0" layoutY="26.0" prefHeight="25.0"
                           prefWidth="442.0" promptText="Cerca..." />
                     </children>
                  </AnchorPane>
                  <Pane layoutX="10.0" layoutY="267.0" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
         </children>
      </HBox>
   </center>
   <top>
      <HBox alignment="CENTER" prefHeight="46.0" prefWidth="641.0" styleClass="top_pane"
         stylesheets="@../css/style.css" BorderPane.alignment="CENTER">
         <children>
            <AnchorPane prefHeight="34.0" prefWidth="207.0">
               <children>
                  <Label layoutY="15.0" prefHeight="17.0" prefWidth="45.0" text="Societa:"
                     textFill="WHITE" />
                  <JFXTextArea editable="false" layoutX="45.0" prefHeight="28.0" prefWidth="162.0" />
               </children>
            </AnchorPane>
            <Pane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="46.0" prefWidth="290.0"
               HBox.hgrow="SOMETIMES" />
            <AnchorPane>
               <children>
                  <Label layoutY="15.0" prefHeight="17.0" prefWidth="45.0" text="Codice:"
                     textFill="WHITE" />
                  <JFXTextArea editable="false" layoutX="45.0" prefHeight="36.0" prefWidth="45.0" />
               </children>
            </AnchorPane>
         </children>
      </HBox>
   </top>
   <bottom>
      <HBox alignment="CENTER">
         <children>
            <JFXButton fx:id="btnAnnulla" contentDisplay="CENTER" ellipsisString="" layoutX="10.0"
               layoutY="10.0" onAction="#eliminaUnitaLocale" prefHeight="44.0" prefWidth="52.0"
               ripplerFill="#ee1f1f" styleClass="bordered" stylesheets="@../css/style.css"
               textFill="WHITE">
               <graphic>
                  <ImageView fitHeight="32.0" fitWidth="30.0" pickOnBounds="true"
                     preserveRatio="true">
                     <image>
                        <Image url="@../icons/cross.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
            <Pane prefHeight="53.0" prefWidth="487.0" />
            <JFXButton fx:id="btnSalva" contentDisplay="CENTER" ellipsisString=""
               onAction="#salvaUnitaLocale" prefHeight="46.0" prefWidth="53.0" ripplerFill="#6ed333"
               styleClass="bordered" stylesheets="@../css/style.css" textFill="WHITE">
               <graphic>
                  <ImageView fitHeight="41.0" fitWidth="41.0" pickOnBounds="true"
                     preserveRatio="true">
                     <image>
                        <Image url="@../icons/save_icon.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
         </children>
      </HBox>
   </bottom>
</BorderPane>