<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="321.0" prefWidth="391.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.Controllers.Home">
   <center>
      <HBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER" prefHeight="218.0" prefWidth="538.0" stylesheets="@../css/style.css">
               <children>
                  <AnchorPane prefHeight="72.0" prefWidth="600.0">
                     <children>
                        <JFXComboBox fx:id="cercaSocieta" editable="true" focusColor="#ffffff00" layoutX="146.0" layoutY="24.0" onHiding="#onSelectedSocieta" prefHeight="25.0" prefWidth="295.0" styleClass="rounded-md" stylesheets="@../css/style.css" unFocusColor="#ffffff00">
                           <padding>
                              <Insets left="10.0" />
                           </padding></JFXComboBox>
                        <Label layoutX="14.0" layoutY="24.0" text="Società - Ente">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <VBox.margin>
                        <Insets right="20.0" />
                     </VBox.margin>
                  </AnchorPane>
                  <Pane layoutX="10.0" layoutY="10.0" prefHeight="8.0" prefWidth="600.0" />
                  <AnchorPane prefHeight="72.0" prefWidth="600.0">
                     <children>
                        <JFXComboBox fx:id="cercaUnitaLocale" editable="true" focusColor="#ffffff00" layoutX="146.0" layoutY="24.0" prefHeight="25.0" prefWidth="295.0" styleClass="rounded-md" stylesheets="@../css/style.css" unFocusColor="#ffffff00">
                           <padding>
                              <Insets left="10.0" />
                           </padding></JFXComboBox>
                        <Label layoutX="14.0" layoutY="24.0" text="Unità Locale">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </AnchorPane>
               </children>
            </VBox>
         </children>
      </HBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <JFXButton fx:id="btnValutaRischi" onAction="#goToValutaRischi" prefHeight="55.0" prefWidth="110.0" stylesheets="@../css/style.css" text="Valutazione Rischi" textAlignment="CENTER" wrapText="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Arial Black" size="14.0" />
               </font></JFXButton>
            <Pane prefHeight="51.0" prefWidth="212.0" HBox.hgrow="SOMETIMES" />
            <JFXButton fx:id="btnScadenze" onAction="#showScadenze" prefHeight="55.0" prefWidth="110.0" stylesheets="@../css/style.css" text="Scadenze">
               <HBox.margin>
                  <Insets bottom="10.0" right="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Arial Black" size="14.0" />
               </font></JFXButton>
         </children>
      </HBox>
   </bottom>
</BorderPane>
